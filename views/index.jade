extends layout

block body
    div
        h2 pablo
            ul
                li
                    label(for='token') Server Token
                    input(type=text, ng-model='server.token', name='token')
                li
                    label(for='startDate') Start Date
                    input(type=text, ng-model='server.startDate', name='startDate')
                li
                    label(for='endDate') End Date
                    input(type=text, ng-model='server.endDate', default='mm/dd/yy', name='endDate')
        a(href='#', ng-click='fetch(server)') Get Data
    div(ng-view)

    script(src='/js/angular.js')
    script(src='/js/app.js')
    script(src='/js/services.js')
    script(src='/js/controllers.js')
    script(src='/js/filters.js')
    script(src='/js/directives.js')
